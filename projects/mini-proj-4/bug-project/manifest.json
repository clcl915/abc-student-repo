{
  "name": "Hehe Doraemon",
  "description": "Infest your page with hehe Doraemon",
  "version": "1.0",
  "manifest_version": 2,
  "permissions":[
    "activeTab"
  ],
  "content_scripts":[
    {
    "matches":["<all_urls>"],
    "js":["contentscript.js"],
    "css":["style.css"]
    }
  ],
  "web_accessible_resources": [
       "hehe-doraemon.png"
   ],
  "browser_action":{
    "default_icon":"clown.png",
    "default_popup":"popup/popup.html"
  }
}
